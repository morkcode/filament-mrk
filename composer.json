{
    "name": "morkcode/filament-mrk",
    "type": "package",
    "description": "filament-mrk Personal addons for Filament",
    "keywords": [
        "laravel",
        "filament",
        "filament-mrk"
    ],
    "homepage": "https://github.com/morkcode/filament-mrk",
    "license": "MIT",
    "authors": [
        {
            "name": "Mork",
            "email": "name@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "FilamentMrk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilamentMrk\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentMrk\\FilamentMrkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
